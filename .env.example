# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# MySQL Database Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_USERNAME=your_username_here
MYSQL_PASSWORD=your_secure_password_here

# Application Environment
ENVIRONMENT=dev
# Options: dev, prod

# Production-only settings (ignore in dev mode)
# SSL Configuration (for production HTTPS)
SSL_KEYSTORE_PASSWORD=your_ssl_keystore_password

# JVM Memory Settings (for production optimization)
JAVA_OPTS=-Xmx512m -Xms256m

# AWS S3 Configuration (if using S3 for storage)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# JWT Configuration (if using JWT for authentication)
JWT_SECRET_KEY=your_jwt_secret_key

# External service URLs (if needed)
# API_BASE_URL=https://your-domain.com/api
